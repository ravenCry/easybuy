    <servlet-mapping>
        <servlet-name>EbShoppingController</servlet-name>
        <url-pattern>/shopping.do</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>all.do</welcome-file>
    </welcome-file-list>
    
</web-app>